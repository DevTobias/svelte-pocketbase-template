    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@modules/eslint-config": "^1.0.0",
    "@modules/prettier-config": "^1.0.0",
    "@sveltejs/adapter-node": "^1.0.0-next.104",
    "@sveltejs/kit": "next",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "prettier": "@modules/prettier-config",
  "eslintConfig": {
    "extends": [
      "@modules/eslint-config"
    ]
  }
}
